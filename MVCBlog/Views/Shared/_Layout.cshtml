<!DOCTYPE html>
<html> 
<head>
   <meta charset="utf-8">
   <title>@ViewBag.Title</title>
   <meta name="viewport" content="width=device-width, initial-scale=1">
   
  <!-- Stylesheets -->
  <link rel="stylesheet" type="text/css"
    href="@Url.Content("~/Content/themes/base/css/main.css")" />
  <link rel="stylesheet" type="text/css"
    href="@Url.Content("~/Content/themes/base/css/normalize.css")" />
  <link rel="stylesheet" type="text/css"
    href="@Url.Content("~/Content/themes/base/css/style.css")" />

  <!-- JavaScript -->
  <script src="@Url.Content("~/Content/themes/base/js/vendor/modernizr-2.6.2.min.js")"></script>
  <script src="@Url.Content("~/Content/themes/base/js/vendor/jquery-1.10.2.min.js")"></script>
  <script src="@Url.Content("~/Content/themes/base/js/main.js")"></script>

</head>
 
<body>
